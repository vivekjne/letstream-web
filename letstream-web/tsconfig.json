{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      
      "jsx": "react",
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true
   },
   "exclude": [
      "client"
   ]
}